{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["history"],
  "background": {
    "service_worker": "background.js"
    },
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },

  "content_security_policy": "script-src 'self' https://developer.mozilla.org; object-src 'self'"
}
  
